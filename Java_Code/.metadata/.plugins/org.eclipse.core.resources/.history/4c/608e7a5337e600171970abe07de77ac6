package Assignment_05_student;

public class TotalDemo extends InputDemo
{
	public void display()
	   {
		   super.marks();
		   for(int i=0;i<5;i++)
		   {
			   sum=0;
			   for(int j=0;j<5;j++)
			   {
				  sum+= arr2[i][j];
			   }
			   sSum[i]=sum;
			   System.out.println("result is summation : "+sSum[i]);
		   }
		   
		   System.out.println("It's TotalDemo Class.First student name is :  "+arName[0]);
		   
	   }
}
