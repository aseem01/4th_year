package button;

import javax.swing.JButton;
import javax.swing.JFrame;

public class normal_button {

	public static void main(String[] args) {
		JFrame f = new JFrame();
		JButton b = new JButton();
		b.setBounds(130, 100, 100, 200);
		f.add(b);
		f.setSize(300, 400);
		f.setLayout(null);
		f.setVisible(true);

	}

}
