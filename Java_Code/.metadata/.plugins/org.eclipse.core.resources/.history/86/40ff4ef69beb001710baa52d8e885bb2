package button;

import javax.swing.ImageIcon;
import javax.swing.JButton;
import javax.swing.JFrame;

public class image_adding_in_button {
	
	image_adding_in_button()
	{
		
		//JFrame Creating;
		JFrame f=new JFrame("Button Example");
		f.setSize(1000, 1000);
		
		JButton b=new JButton(new ImageIcon("D:\\4th_year\\Java_Code\\Java_Swing_Practices\\src\\button\\register.png"));
	    b.setBounds(100, 100, 758, 262);
	    
	    f.add(b);
	    
	    f.setLayout(null);
	    f.setVisible(true);
	    
	}

	public static void main(String[] args) 
	{
		
         new image_adding_in_button();
	}

}
